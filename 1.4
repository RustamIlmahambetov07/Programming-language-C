#include <stdio.h>

#define LOWER 0 /* нижняя граница таблицы */
#define UPPER 300 /* верхняя строка */
#define STEP 20 /* размер шага*/

int main() {

    int fahr;

    for (fahr = LOWER; fahr <= UPPER; fahr = fahr + STEP) {
        printf("%3.0d %6.1f\n", fahr, (5.0 / 9.0) * (fahr - 32));
    }

}
